<div class="page-header">
  <h3>Favorite Food #<%= @favorite_food.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Favoritefood name</dt>
      <dd><%= @favorite_food.favoritefood_name %></dd>

      <dt>Foods</dt>
      <dd><%= @favorite_food.foods_id %></dd>

      <dt>User</dt>
      <dd><%= @favorite_food.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/favorite_foods" class="btn btn-primary">
        Back
      </a>
      <a href="/favorite_foods/<%= @favorite_food.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_favorite_food/<%= @favorite_food.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
