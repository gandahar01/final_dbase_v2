<div class="page-header">
  <h1>
    Favorite foods
    <a href="/favorite_foods/new" class="btn btn-lg btn-success">New Favorite food</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Favoritefood name</th>
        <th>Foods</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @favorite_foods.each do |favorite_food| %>
      <tr>
        <td><%= favorite_food.favoritefood_name %></td>
        <td><%= favorite_food.foods_id %></td>
        <td><%= favorite_food.user_id %></td>
        <td>
          <a href="/favorite_foods/<%= favorite_food.id %>" class="btn btn-primary">Show</a>
          <a href="/favorite_foods/<%= favorite_food.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_favorite_food/<%= favorite_food.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
