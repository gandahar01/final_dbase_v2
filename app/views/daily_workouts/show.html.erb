<div class="page-header">
  <h3>Daily Workout #<%= @daily_workout.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Workout date</dt>
      <dd><%= @daily_workout.workout_date %></dd>

      <dt>Dailyworkout check</dt>
      <dd><%= @daily_workout.dailyworkout_check %></dd>

      <dt>User</dt>
      <dd><%= @daily_workout.user_id %></dd>

      <dt>Weeklyworkout</dt>
      <dd><%= @daily_workout.weeklyworkout_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/daily_workouts" class="btn btn-primary">
        Back
      </a>
      <a href="/daily_workouts/<%= @daily_workout.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_daily_workout/<%= @daily_workout.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
