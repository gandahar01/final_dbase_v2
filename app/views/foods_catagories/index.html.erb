<div class="page-header">
  <h1>
    Foods catagories
    <a href="/foods_catagories/new" class="btn btn-lg btn-success">New Foods catagory</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Foodcatagory name</th>
        <th>Actions</th>
      </tr>

      <% @foods_catagories.each do |foods_catagory| %>
      <tr>
        <td><%= foods_catagory.foodcatagory_name %></td>
        <td>
          <a href="/foods_catagories/<%= foods_catagory.id %>" class="btn btn-primary">Show</a>
          <a href="/foods_catagories/<%= foods_catagory.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_foods_catagory/<%= foods_catagory.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @foods_catagories, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
