<div class="page-header">
  <h1>
    Foods nutritions
    <a href="/foods_nutritions/new" class="btn btn-lg btn-success">New Foods nutrition</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Foods</th>
        <th>Foodscatagory</th>
        <th>Foods calories</th>
        <th>Foods carbs</th>
        <th>Foods fat</th>
        <th>Foods sugar</th>
        <th>Actions</th>
      </tr>

      <% @foods_nutritions.each do |foods_nutrition| %>
      <tr>
        <td>
          <% if foods_nutrition.foods.present? %>
            <a href="/foods/<%= foods_nutrition.foods.id %>">
              <%= foods_nutrition.foods.food_name %>
            </a>
          <% end %>
        </td>
        <td>
          <% if foods_nutrition.foodscatagory.present? %>
            <a href="/foods_catagories/<%= foods_nutrition.foodscatagory.id %>">
              <%= foods_nutrition.foodscatagory.created_at %>
            </a>
          <% end %>
        </td>
        <td><%= foods_nutrition.foods_calories %></td>
        <td><%= foods_nutrition.foods_carbs %></td>
        <td><%= foods_nutrition.foods_fat %></td>
        <td><%= foods_nutrition.foods_sugar %></td>
        <td>
          <a href="/foods_nutritions/<%= foods_nutrition.id %>" class="btn btn-primary">Show</a>
          <a href="/foods_nutritions/<%= foods_nutrition.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_foods_nutrition/<%= foods_nutrition.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @foods_nutritions, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
